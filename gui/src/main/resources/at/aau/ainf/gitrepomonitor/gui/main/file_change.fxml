<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="container" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <ImageView fx:id="iconChange" fitHeight="18.0" fitWidth="109.0" pickOnBounds="true" preserveRatio="true">
      <Image url="@../icons/icon_attention.png"/>
   </ImageView>
   <Label fx:id="lblFileName" text="File name" HBox.hgrow="ALWAYS">
      <HBox.margin>
         <Insets left="5.0"/>
      </HBox.margin>
   </Label>
</HBox>
