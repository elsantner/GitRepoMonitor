<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="400.0" stylesheets="@auth_list.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.aau.ainf.gitrepomonitor.gui.auth.ControllerAuthList">
   <HBox alignment="CENTER" layoutX="10.0" layoutY="93.0" spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
      <VBox alignment="TOP_CENTER" spacing="5.0" HBox.hgrow="ALWAYS">
         <Label text="%lbl.https" textAlignment="CENTER">
            <graphic>
               <Button mnemonicParsing="false" onAction="#onBtnAddHttps">
                  <graphic>
                     <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../icons/icon_plus.png"/>
                     </ImageView>
                  </graphic>
               </Button>
            </graphic></Label>
         <ListView fx:id="listHTTPS" prefWidth="250.0" VBox.vgrow="ALWAYS" />
      </VBox>
      <VBox alignment="TOP_CENTER" spacing="5.0" HBox.hgrow="ALWAYS">
         <Label text="%lbl.ssl" textAlignment="CENTER">
            <graphic>
               <Button mnemonicParsing="false" onAction="#onBtnAddSsl">
                  <graphic>
                     <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../icons/icon_plus.png"/>
                     </ImageView>
                  </graphic>
               </Button>
            </graphic></Label>
         <ListView fx:id="listSSL" prefWidth="250.0" VBox.vgrow="ALWAYS" />
      </VBox>
   </HBox>
</AnchorPane>
